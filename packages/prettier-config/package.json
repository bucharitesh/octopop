{
  "name": "@octopop/prettier-config",
  "version": "0.0.1",
  "license": "MIT",
  "main": ".prettierrc.json",
  "scripts": {
    "lint": "prettier -c --cache ."
  },
  "devDependencies": {
    "lint-staged": "13.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    ".prettierrc.json"
  ],
  "keywords": [
    "prettier",
    "config",
    "prettier-config"
  ],
  "lint-staged": {
    "**/*": "prettier -w -u --cache"
  }
}